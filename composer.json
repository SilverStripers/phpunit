{
    "name": "sminnee/phpunit",
    "description": "The PHP Unit Testing framework - forked to backport PHP 8 support",
    "type": "library",
    "keywords": [
        "phpunit",
        "xunit",
        "testing"
    ],
    "homepage": "https://phpunit.de/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sminnee/phpunit/issues"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "doctrine/instantiator": "^1.3.1",
        "myclabs/deep-copy": "^1.10.1",
        "phar-io/manifest": "^2.0.1",
        "phar-io/version": "^3.0.2",
        "phpunit/php-code-coverage": "^10.0",
        "phpunit/php-file-iterator": "^4.0",
        "phpunit/php-invoker": "^4.0",
        "phpunit/php-text-template": "^2.0.3",
        "phpunit/php-timer": "^6.0",
        "phpspec/prophecy": "^1.6.2",
        "sebastian/cli-parser": "^2.0",
        "sebastian/code-unit": "^2.0",
        "sebastian/comparator": "^4.0.6",
        "sebastian/diff": "^4.0.3",
        "sebastian/environment": "^5.1.3",
        "sebastian/exporter": "^4.0",
        "sebastian/global-state": "^5.0.1",
        "sebastian/object-enumerator": "^4.0.3",
        "sebastian/resource-operations": "~3.0",
        "sebastian/type": "^3.0",
        "sebastian/version": "^4.0",
        "sminnee/phpunit-mock-objects": "dev-php8-support",
        "symfony/yaml": "~2.1|~3.0|~4.0"
    },
    "replace": {
        "phpunit/phpunit": "^5.7"
    },
    "require-dev": {
        "ext-PDO": "*"
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "suggest": {
        "ext-soap": "*"
    },
    "bin": [
        "phpunit"
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/Framework/Assert/Functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "files": [
            "tests/_files/CoverageNamespacedFunctionTest.php",
            "tests/_files/CoveredFunction.php",
            "tests/_files/NamespaceCoveredFunction.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.7.x-dev"
        },
        "repositories": {
            "phpunit/phpunit-mock-objects": {
                "type": "git",
                "url": "git@github.com:SilverStripers/phpunit-mock-objects.git"
            }
        }
    }
}
